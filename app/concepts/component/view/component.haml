- if @static
  = render_content
- else
  %component{"is": @component_class, "id": "#{options[:component_key]}", "class": @component_class, ":params":  @url_params.to_json, ":component-config": @component_config.to_json, "inline-template": true}
    %div
      %div{"v-if": "asyncTemplate == null"}
        = render_content
      %div{"v-if": "asyncTemplate != null"}
        %v-runtime-template{":template":"asyncTemplate"}
